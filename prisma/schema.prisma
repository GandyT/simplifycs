// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Verify {
  email String @unique
  username String @unique
  firstName String
  lastName String
  password String
}

model User {
  id Int @id @default(autoincrement())
  email String @unique
  username String @unique
  firstName String
  lastName String
  password String
}
